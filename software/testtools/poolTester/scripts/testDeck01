# Restart thread.
thread:restart, repetitions:infinite
operation_on_thread:restart, run:sleep 20
operation_on_thread:restart, run:restart

# Post-Execs
post_exec:dumpStats
post_exec:generationStats
post_exec:workerStats
post_exec:stop
